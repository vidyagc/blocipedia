
<h3>Add a Collaborator</h3>
 <%= form_for [wiki, collaborator] do |f| %>
   <div class="form-group row">
    <div class="col-xs-3">
      
        <div class="form-group">
         <%= f.label :emailid, class: 'sr-only' %>
         <%= f.text_field :emailid, class: 'form-control', placeholder: "Enter email address" %>
        </div>
        <div class="form-group">
         <%= f.submit "Submit Collaborator", class: 'btn btn-info pull-left' %>
        </div>
    </div>
    </div>
 <% end %>
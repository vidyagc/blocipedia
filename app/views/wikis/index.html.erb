 <h1>Wikis</h1>
 <% @wikis.each do |wiki| %>
  <% if wiki.private == false || wiki.user_id == current_user.id || current_user.role == 'admin'%>
   <div class="media">
     <div class="media-body">
       <h4 class="media-heading">
         <%= link_to wiki.title, wiki %>
       </h4>
     </div>
   </div>
  <% end %>
 <% end %>

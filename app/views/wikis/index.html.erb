<h1>Wikis</h1>
<% @wikis.each do |wiki| %>

   <div class="row">
      <div class="col-md-10">
      <div class="media">
         <div class="media-body">
            <h4 class="media-heading">
               <%= link_to markdown(wiki.title), wiki, class: "wrap" %>
            </h4>
         </div>
      </div>
      </div>
   </div>

<% end %>
